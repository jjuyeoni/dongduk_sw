
<h1> 지원서 수정</h1>
<%=form_for(@apply, url: apply_update_path, method: 'patch')do |f| %>
                <%=f.label :title%><br>
	            <%=f.text_field :title , class:"form-control"%> 
	            <%=f.label :content%><br>
	            <%=f.text_field :content , class:"form-control", size: "24x6" %>
        	    <%=f.submit '완료',class:"btn", :style => 'float:right'%>
<%end%>            